{
  "name": "web-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e ../../envs/.env.local next dev",
    "dev:docker": "docker compose up",
    "dev:build": "dotenv -e ../../envs/.env.local next build",
    "dev:start": "dotenv -e ../../envs/.env.local next start",
    "build": "cat ../../envs/.env.prod && dotenv -e ../../envs/.env.prod next build",
    "start": "cat ../../envs/.env.prod && dotenv -e ../../envs/.env.prod next start",
    "public-prod:build": "cat ../../envs/.env.public-prod && dotenv -e ../../envs/.env.public-prod next build",
    "public-prod:start": "dotenv -e ../../envs/.env.public-prod next start",
    "customer-prod:build": "cat ../../envs/.env.customer-prod && dotenv -e ../../envs/.env.customer-prod next build",
    "customer-prod:start": "dotenv -e ../../envs/.env.customer-prod next start",
    "develop:build": "cat ../../envs/.env.dev && dotenv -e ../../envs/.env.dev next build",
    "develop:start": "dotenv -e ../../envs/.env.dev next start",
    "staging:build": "cat ../../envs/.env.stg && dotenv -e ../../envs/.env.stg next build",
    "staging:start": "dotenv -e ../../envs/.env.stg next start",
    "tmp:build": "cat ../../envs/.env.tmp && dotenv -e ../../envs/.env.tmp next build",
    "tmp:start": "dotenv -e ../../envs/.env.tmp next start",
    "lint": "next lint",
    "lint:errors-only": "next lint --quiet",
    "lint:fix": "next lint --fix",
    "format": "eslint --fix src/ && prettier --write src/",
    "formatOnly": "prettier --write src/",
    "dev:studio": "dotenv -e ../../envs/.env.local prisma studio",
    "prod:studio": "dotenv -e ../../envs/.env.prod prisma studio",
    "prepare": "husky",
    "e2e:build": "dotenv -e ../../envs/.env.local next build",
    "e2e:start": "dotenv -e ../../envs/.env.local next start -- -p 3020",
    "test:integ:api:internal": "dotenv -e ../../envs/.env.dev vitest run",
    "test:load:api": "dotenv -e ../../envs/.env.local vitest run -- -c ./vitest.load.config.ts",
    "customApiTest": "dotenv -e ../../envs/.env.local vitest run -- ./src/app/api/store/[store_id]/purchase-table/route.test.ts",
    "test:e2e": "playwright test",
    "test:e2e:minimum": "playwright test tests/e2e/tests/sale/sale-transactions.spec.ts tests/e2e/tests/purchase/purchase-transactions.spec.ts",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:chrome": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit",
    "test:e2e:report": "playwright show-report",
    "prisma:generate": "prisma generate --schema=../backend-core/prisma/schema",
    "postinstall": "playwright install"
  },
